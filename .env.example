# Autodesk Platform Services Credentials
# Get these from https://aps.autodesk.com/ after creating an app
AUTODESK_CLIENT_ID=your_client_id_here
AUTODESK_CLIENT_SECRET=your_client_secret_here
AUTODESK_CALLBACK_URL=http://localhost:3000/callback

# Optional: Specific Account/Hub ID if you want to limit to one account
# AUTODESK_ACCOUNT_ID=your_account_id_here
# AUTODESK_HUB_ID=your_hub_id_here

# Server Configuration
PORT=3000
LOG_LEVEL=info  # Options: error, warn, info, debug

# Optional: Webhook Configuration
# Used for receiving real-time updates from Autodesk Build
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_ENDPOINT=https://your-domain.com/webhooks

# Optional: Cache Configuration
# TTL in seconds, max size in number of entries
CACHE_TTL=3600
CACHE_MAX_SIZE=100
CACHE_CHECK_PERIOD=600

# Optional: Rate Limiting
# Requests per minute
RATE_LIMIT=60

# Optional: Token Storage
# Where to store OAuth tokens (keychain, file, memory)
TOKEN_STORAGE=keychain

# Optional: Development/Debug Settings
DEBUG=false
LOG_API_CALLS=false
MOCK_API=false